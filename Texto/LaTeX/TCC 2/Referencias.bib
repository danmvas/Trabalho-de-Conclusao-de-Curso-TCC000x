    @article{nascimento:2020, title={Fracasso escolar e evasão no Ensino Médio no Brasil: estado do conhecimento}, volume={4}, url={https://periodicos.ifsul.edu.br/index.php/educarmais/article/view/1823}, DOI={10.15536/reducarmais.4.2020.379-393.1823}, abstractNote={&amp;lt;p class=&amp;quot;Normal0&amp;quot;&amp;gt;A presente pesquisa teve como objetivo principal realizar uma revisão (estado do conhecimento) sobre a evasão escolar no Ensino Médio no Brasil, tendo como ênfase o fracasso escolar, de modo a compreender as principais variáveis e aspectos relacionados ao abandono por parte dos alunos deste ciclo na formação básica da escolarização. Foram selecionados artigos nacionais da base de dados Capes Periódicos considerando a última década, utilizando a combinação dos descritores “evasão escolar” e “ensino médio”. Os dados coletados foram submetidos à análise de conteúdo. Os resultados principais evidenciaram que a evasão escolar ocorre, na maioria das vezes, por causa da vulnerabilidade social, falta de adequação das propostas pedagógicas à realidade dos estudantes, ausência de políticas públicas e trabalho precoce.&amp;lt;/p&amp;gt;}, number={2}, journal={Revista Educar Mais}, author={Nascimento, João Carlos de Souza and Nascimento, Edileuza Knupp and Camargo, Daniela Beatriz Pires and da Silva, Tiago Esteves and Azevedo, Tiago Dias Ferracioli and Klumpp, Carolina Ferreira Barros}, year={2020}, month={set.}, pages={379–393} }

